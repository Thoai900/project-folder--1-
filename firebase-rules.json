{
  "rules": {
    "users": {
      "$userId": {
        ".read": "$userId === auth.uid",
        ".write": "$userId === auth.uid"
      }
    },
    "publicUsers": {
      ".read": "auth != null",
      "$userId": {
        ".write": "$userId === auth.uid"
      }
    },
    "prompts": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
